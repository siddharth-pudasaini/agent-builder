{
    "name": "@siddharthapudasaini/agent-builder",
    "version": "1.0.2",
    "description": "A TypeScript library for building AI agents with tool calling capabilities, autonomous orchestration, and LLM integration",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsc",
        "prepublishOnly": "npm run build",
        "test": "jest",
        "test:watch": "jest --watch",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "typecheck": "tsc --noEmit",
        "clean": "rm -rf dist",
        "prepare": "npm run build"
    },
    "keywords": [
        "ai",
        "agent",
        "llm",
        "openai",
        "gpt",
        "tools",
        "orchestrator",
        "automation",
        "typescript",
        "chatgpt"
    ],
    "author": "Siddhartha Pudasaini",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/siddharth-pudasaini/agent-builder.git"
    },
    "bugs": {
        "url": "https://github.com/siddharth-pudasaini/agent-builder/issues"
    },
    "homepage": "https://github.com/siddharth-pudasaini/agent-builder#readme",
    "engines": {
        "node": ">=18.0.0"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "typescript": "^5.3.0"
    },
    "dependencies": {
        "zod": "^4.3.5"
    }
}
